<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_move_tree_extenstion" model="ir.ui.view">
            <field name="name">stock.move.tree</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_tree"/>
            <field eval="8" name="priority"/>
            <field name="arch" type="xml">
                <xpath expr="/tree//field[@name='location_id']" position="replace">
                    <field name="location_id" groups="stock.group_locations" context="{'short_name': True}"/>
                </xpath>
                <xpath expr="/tree//field[@name='location_dest_id']" position="replace">
                    <field name="location_dest_id" groups="stock.group_locations" context="{'short_name': True}"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>

